<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_insufficient_warning_view_form" model="ir.ui.view">
        <field name="name">stock.insufficient.warning.view.form</field>
        <field name="model">stock.insufficient.warning</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="package_id" invisible="1" />
                    <field name="line_ids" />
                </sheet>
                <footer>
                    <button string="CONFIRM" name="btn_confirm" type="object" class="oe_highlight" />
                    -
                    <button string="CANCEL" special="cancel" type="object" class="btn-danger" />
                </footer>
            </form>
        </field>
    </record>

</odoo>